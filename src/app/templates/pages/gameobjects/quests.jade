section.page
  div.clearfix
    div(data-hook="game-object-menu")

  h2.section-title Quests

  .buttons.btn-group
    button.btn.btn-default(data-hook="fetch") Refresh 

  table.table.table-striped.table-hover
    thead
      tr
        th
          span() Id
        th
          span() Name
        th
          span() Group
        th
          span() SubTitle
        th
          span() Difficulty
        th
          span() Icon
        th
          span() Repeatable
        th
          span() Category
        th
          span() Actions
        th
          span() Requirements
        th
          span() Objectives
        th
          span() Rewards
    tbody(data-hook="quest-list")

